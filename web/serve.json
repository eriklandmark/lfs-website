{
    "public": "dist",
    "rewrites": [
        {
            "source": "/**",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "**/*.@(jpg|jpeg|gif|png|avif)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=1209600"
                }
            ]
        }
    ]
}